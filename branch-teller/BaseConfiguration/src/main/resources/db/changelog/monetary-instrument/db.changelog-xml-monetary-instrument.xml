                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1" author="zahra">
        <createTable tableName="BT_MONETARY_INSTRUMENT">
            <column autoIncrement="true" defaultOnNull="false" name="MONETARY_INSTRUMENT_ID" type="NUMBER(19, 0)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="CODE" type="VARCHAR2(10 CHAR)">
                <constraints nullable="false"/>
            </column>
            <column name="CURRENCY" type="VARCHAR2(3 CHAR)">
                <constraints nullable="false"/>
            </column>
            <column name="IS_CASH" type="NUMBER(1, 0)">
                <constraints nullable="false"/>
            </column>
            <column name="IS_DELETED" type="NUMBER(1, 0)" defaultValueNumeric="0" />
            <column name="TRANSIT_ACCOUNT" type="VARCHAR2(20 CHAR)"/>
            <column name="VERSION_NUM" type="NUMBER(10, 0)"/>

